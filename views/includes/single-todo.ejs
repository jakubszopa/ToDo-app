<article class="todo-container">
    <div class="todo-check">
        <% if (item.status === 1) { %> 
            <svg xmlns="http://www.w3.org/2000/svg" width="11" height="9">
            <path
                fill="none"
                stroke="#FFF"
                stroke-width="2"
                d="M1 4.304L3.696 7l6-6"
            />
            </svg>
        <% } %> 
      </div>
      <p class="single-task-p" data-id="<%= item._id %>" data-status="<%= item.status %>"><%= item.task %></p>
</article>